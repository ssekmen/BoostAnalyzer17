  select size(JetAK4) >= 3

region SR_Lep_V_0b
  select Pre_Lep
  select size(HadV) >= 1
  select size(LooseIsoBTag) == 0
  select size(HadTop) == 0
  select size(JetAK4) >= 3

region SR_Lep_H_b
  select Pre_Lep
  select size(HadH) >= 1
  select size(LooseIsoBTag) >= 1
  select size(HadTop) == 0
  select size(JetAK4) >= 3

region SR_Lep_H_0b
  select Pre_Lep
  select size(HadH) >= 1
  select size(LooseIsoBTag) == 0
  select size(HadTop) == 0
  select size(JetAK4) >= 3

# Non-isolated lepton signal regions
region SR_Leptop_0htop
