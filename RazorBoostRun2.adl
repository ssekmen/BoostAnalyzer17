  select size(LepJet) >= 1
  select size(HadV) == 0
  select size(LepTop) == 0
  select size(HadTop) == 0
  select size(JetAK4) >= 5

region SR_Lepjet_1V_24j
  select Pre_NonIso
  select size(LepJet) >= 1
  select size(HadV) >= 1
  select size(LepTop) == 0
  select size(HadTop) == 0
